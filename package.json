{
  "name": "react-maxwell-modal",
  "version": "2.0.5",
  "description": "A react component for awesome modals",
  "main": "lib/index.js",
  "scripts": {
    "test": "nyc ava src/**.test.js ",
    "clean": "rimraf lib dist",
    "lint": "node_modules/.bin/eslint src/**.js",
    "build:lib": "babel src --out-dir lib",
    "build:example": "cd example && npm install",
    "build": "npm run build:lib",
    "check": "npm run lint",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "./setup-test-env.js"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "author": "James Hrisho",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.14.0",
    "babel": "6.5.2",
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-register": "6.8.0",
    "coveralls": "2.11.9",
    "enzyme": "2.3.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-plugin-react": "^3.13.1",
    "jsdom": "6.5.1",
    "nyc": "6.4.4",
    "react-addons-test-utils": "15.0.2",
    "react-dom": "15.0.2",
    "rimraf": "2.5.2",
    "sinon": "^1.17.3",
    "webpack": "^1.13.0"
  },
  "keywords": [
    "modal",
    "react-component",
    "alert",
    "confirm",
    "maxwellhealth"
  ],
  "dependencies": {
    "lodash": "^4.11.1",
    "react-modal": "1.2.1"
  },
  "peerDependencies": {
    "react": "^0.13.0 || ^0.14.0 || ^15.0.0"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/maxwellhealth/react-maxwell-modal.git"
  }
}
